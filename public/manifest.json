{
  "manifest_version": 3,
  "name": "FRC Helper",
  "description": "Helps transfer data from School Interviews to Aspen",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["js/add_new_family.js", "js/bootstrap.bundle.min.js"],
      "css": ["css/bootstrap.min.css"],
      "matches": [
        "https://www.schoolinterviews.com.au/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["html/add_new_family.html"],
      "matches": ["https://www.schoolinterviews.com.au/*"]
    }
  ]
}
